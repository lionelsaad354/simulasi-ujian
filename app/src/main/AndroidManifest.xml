                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityKuis1"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ActivityKuis2"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ActivityKuis3"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ActivityKuis4"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ActivityKuis5"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Profileku"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Main2Activity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".TentangAplikasi"
            android:parentActivityName=".MainActivity"/>
